{
  "name": "electron-react-starter",
  "version": "1.1.0",
  "description": "electron-react-starter",
  "main": "build/bundle/main.js",
  "scripts": {
    "nodev": "nvm install 14.17.0",
    "format": "npx prettier --write . && npx eslint --fix --cache .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "cross-env NODE_ENV=development node ./config/dev.js",
    "dev": "npm run serve",
    "dist": "cross-env NODE_ENV=production node ./config/dist.js",
    "build:now": "cross-env electron-builder --config electron.builder.js",
    "build": "npm run dist && electron-builder build --config electron.builder.js",
    "build:windows": "npm run dist && electron-builder build --config electron.builder.js --win",
    "build:mac": "npm run dist && electron-builder build --config electron.builder.js --mac",
    "build:linux": "npm run dist && electron-builder build --config electron.builder.js --linux",
    "lint:fix": "eslint --max-warnings 100 --fix --cache src --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint --max-warnings 100 --cache src --ext .js,.jsx,.ts,.tsx",
    "npm:update": "npm-check-updates -u",
    "npm:check": "npm-check-updates",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnyzak/electron-react-starter.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack"
  ],
  "author": "funnyzak <silenceace@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/funnyzak/electron-react-starter/issues"
  },
  "homepage": "https://github.com/funnyzak/electron-react-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.20.2",
    "@emotion/eslint-plugin": "^11.10.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-loader": "^8.2.5",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "handlebars": "^4.7.7",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "micromatch": "^4.0.5",
    "node-sass": "^8.0.0",
    "npm-check-updates": "^16.5.1",
    "prettier": "2.7.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "antd": "^5.0.4",
    "core-js": "^3.27.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}